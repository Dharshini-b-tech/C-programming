int main() {
    char n;
    scanf("%c",&n);
    if(n>='A' && n<='Z' || n>='a' && n<='z'){
        if(n=='a'||'e'||'i'||'o'||'u'||'A'||'E'||'I'||'O'||'U'){
            printf("%c is a vowel",n);
        }
        else{
            printf("%c is consonant",n);
        }
    }
    else{
        printf("%c is not a letter",n);
    }
    return 0;

}
