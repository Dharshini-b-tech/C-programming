#include <stdio.h>

int main(){
    int a,b,ch;
    scanf("%d",&ch);
    scanf("%d %d",&a,&b);

    if(ch==1){
        printf("result = %d",a+b);
    }
    else if(ch==2){
        printf("result = %d",a-b);
    }
    else if(ch==3){
        printf("result = %d",a*b);
    }
    else if(ch==4){
        printf("result = %d",a/b);
    }
    else{
        printf("invalid value");
    }

    return 0;
}
